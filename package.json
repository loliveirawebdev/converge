{
  "private": true,
  "scripts": {
    "web:local": "next start",
    "postinstall": "npx lefthook install",
    "storybook": "npx start-storybook -p 6006",
    "web:dev": "yarn file:generation && next",
    "web:build": "yarn file:generation && next build",
    "mobile:dev": "yarn file:generation && expo start",
    "file:generation": "yarn generate:webroute:files && yarn generate:module:instances",
    "generate:webroute:files": "npx ts-node ./modules/Routing/scripts/generateWebRoutes.ts",
    "generate:module:instances": "npx ts-node ./modules/Routing/scripts/instantiateMobileModules.ts"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "expo": "~45.0.0",
    "expo-localization": "~13.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "intl": "^1.2.5",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "next": "12.2.2",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-optimized-images": "^2.6.2",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^6.0.5",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-web": "0.17.7",
    "setimmediate": "~1.0.5",
    "svg-sprite-loader": "^6.0.11",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@evilmartians/lefthook": "^1.0.5",
    "@expo/next-adapter": "3.1.20",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-react-native-web": "0.0.15",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.18.7",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.3",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "17",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./modules/**/*.{js,ts,jsx,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 1
      }
    }
  }
}
